{
  "name": "react-foundation",
  "version": "0.1.9",
  "description": "Foundation as React components.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src",
    ".npmignore",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "compile": "babel --loose es6.modules -d lib/ src/",
    "lint": "eslint src test",
    "prepublish": "npm run compile",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require test/index.js --recursive",
    "test:watch": "npm test -- --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nordsoftware/react-foundation.git"
  },
  "keywords": [
    "badge",
    "button",
    "callout",
    "video",
    "grid",
    "icon",
    "label",
    "menu",
    "progress-bar",
    "responsive",
    "switch",
    "top-bar",
    "react",
    "foundation",
    "react-component"
  ],
  "author": "Christoffer Niska <christofferniska@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nordsoftware/react-foundation/issues"
  },
  "homepage": "https://github.com/nordsoftware/react-foundation#readme",
  "dependencies": {
    "check-types": "^6.0.0",
    "classnames": "^2.2.3",
    "lodash.has": "^4.2.1",
    "lodash.omit": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-eslint": "^5.0.0",
    "babel-plugin-module-alias": "^1.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "chai-jsx": "^1.0.1",
    "cheerio": "^0.20.0",
    "enzyme": "^2.1.0",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^4.2.1",
    "estraverse-fb": "^1.3.1",
    "expect": "^1.15.1",
    "foundation-sites": "^6.2.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "sinon": "^1.17.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      [
        "babel-plugin-module-alias",
        [
          {
            "src": "./src/components",
            "expose": "components"
          }
        ]
      ]
    ]
  }
}
